    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/script/jest-setup.js"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^16.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-react-native": "^1.9.0",
    "bookshelf": "^0.10.2",
    "bookshelf-paranoia": "^0.9.0",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "express": "^4.14.0",
    "express-graphql": "^0.5.4",
    "faker": "^3.1.0",
    "graphql": "^0.7.1",
    "graphql-relay": "^0.4.3",
    "jest": "^16.0.1",
    "jsonwebtoken": "^7.1.9",
    "knex": "^0.12.2",
    "lodash": "^4.16.4",
    "moment": "^2.15.1",
    "nodemon": "^1.11.0",
    "pg": "^6.1.0",
    "query-string": "^4.2.3",
    "slack": "^8.2.0"
  }
