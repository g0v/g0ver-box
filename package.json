{
  "name": "g0ver-box",
  "version": "1.0.0",
  "description": "A macthmaking slack-bot for g0v projects and g0v attendees.",
  "main": "./dist/app.js",
  "scripts": {
    "start": "node ./dist/app.js",
    "watch": "export NODE_ENV=development; nodemon --exec npm run start",
    "schema": "babel-node ./script/migration-schema.js",
    "build": "rm -rf dist; babel src -d dist --ignore '/__tests__/,/__mocks__/'",
    "heroku-postbuild": "npm run migrate; npm run build;",
    "test": "jest --coverage",
    "eslint": "eslint ./src",
    "migrate": "knex migrate:latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/g0v/g0ver-box.git"
  },
  "author": "g0v",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/g0v/g0ver-box/issues"
  },
  "homepage": "https://github.com/g0v/g0ver-box#readme",
  "jest": {
    "testEnvironment": "node",
    "clearMocks": true,
    "setupFiles": [
      "<rootDir>/__mocks__/index.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-mock-knex/serializer"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/migrations/"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^16.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-react-native": "^1.9.0",
    "cache-model": "^1.9.3",
    "cz-conventional-changelog": "^1.2.0",
    "dataloader": "^1.3.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "express": "^4.14.0",
    "express-graphql": "^0.5.4",
    "faker": "^3.1.0",
    "graphql": "^0.7.1",
    "graphql-relay": "^0.4.3",
    "graphql-tower": "^2.17.3",
    "jest": "^21.2.1",
    "jest-mock-knex": "^1.7.5",
    "jsonwebtoken": "^7.1.9",
    "knex": "^0.13.0",
    "lodash": "^4.16.4",
    "moment": "^2.15.1",
    "nodemon": "^1.11.0",
    "pg": "^7.3.0",
    "query-string": "^4.2.3",
    "slack": "^8.2.0",
    "xregexp": "^3.2.0"
  }
}
