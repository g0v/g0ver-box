  "repository": {
    "type": "git",
    "url": "git+https://github.com/g0v/hub.git"
  },
  "author": "g0v",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/g0v/hub/issues"
  },
  "homepage": "https://github.com/g0v/hub#readme",
